{
  "manifest_version": 2,
  "name": "Strong Password Generator",
  "version": "1.0.0",
  "description": "Strong password generator",
  "icons": {
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "logo512.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contentSettings",
    "desktopCapture",
    "*://*/*",
    "tabCapture",
    "tabs",
    "tts",
    "ttsEngine"
  ]
}
